                    itemSelector: '.category',
                    layoutMode: 'masonry'
                });
                */
                $("select.filters_masonry").on('change', function() {
                    var filter = $(this).val();
                    //$grid.isotope({ filter: '.'+filter });
                    window.top.location.href= 'https://sitgesfilmfestival.com/cas/programa/pel_licules?filter='+filter;
                                    });
                $("select.filters_masonry2").on('change', function() {
                    var filter = $(this).val();
                    //$grid.isotope({ filter: '.'+filter });
                    window.top.location.href= 'https://sitgesfilmfestival.com/cas/programa/pel_licules?filmsperpage='+filter+'';
                });

                $(function() {
                    //$('.category-inner-image').matchHeight();
                    $('.right-banner-bottom').matchHeight();
                });
                $(".category-inner-image").click(function () {
                    var rel = $(this).attr("rel");
                    window.top.location.href= rel;
                });

            });

