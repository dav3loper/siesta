    <script>
        if(!alertify.privacyAlert){
            //define a new errorAlert base on alert
            alertify.dialog('privacyAlert',function factory(){
                return{
                    build:function(){
                        var errorHeader = 'Pol&iacute;tica de privacidad';
                        this.setHeader(errorHeader);
                    },
                    setup:function(){
                        return {
                            buttons:[{text: "ok!", key:27/*Esc*/}],
                            options: {
                                onshow:function(){
                                    $(".ajs-modal").addClass("ajs-legal");
                                },
                                maximizable: false,
                                closable: false,
                                movable: false
                            }
                        };
                    }


                };
            },true,'alert');
            //alertify
        }
        if(!alertify.legalAlert){
            //define a new errorAlert base on alert
            alertify.dialog('legalAlert',function factory(){
                return{
                    build:function(){
                        var errorHeader = 'Pol&iacute;tica de privacidad';
                        this.setHeader(errorHeader);
                    },
                    setup:function(){
                        return {
                            buttons:[{text: "ok!", key:27/*Esc*/}],
                            options: {
                                onshow:function(){
                                    $(".ajs-modal").addClass("ajs-legal");
                                },
                                maximizable: false,
                                closable: false,
                                movable: false
                            }
                        };
                    }


                };
            },true,'alert');
        }

        if(!alertify.popupNewsletter){
            //define a new errorAlert base on alert
            alertify.dialog('popupNewsletter',function factory(){
                return{
                    build:function(){
                        var errorHeader = 'Newsletter del Festival de Sitges';
                        this.setHeader(errorHeader);
                    },
                    setup:function(){
                        return {
                            //buttons:[{text: "ok!", key:27/*Esc*/}],
                            options: {
                                onshow:function(){
                                    $(".ajs-modal").addClass("ajs-popup_newsletter");
                                },
                                maximizable: false,
                                closable: true,
                                movable: false
                            }
                        };
                    }


                };
            },true,'alert');
            //alertify
        }

