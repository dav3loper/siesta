{
  "name":"siesta/siesta",
  "description": "Sistema Inteligente para Elegir en Sitges Todo lo Acordado",
  "type": "project",
  "require": {
    "ext-json": "*",
    "fideloper/proxy": "^4.0",
    "imangazaliev/didom": "^1.13",
    "laravel/framework": "5.8",
    "laravel/tinker": "^1.0"
  },
  "require-dev": {
    "filp/whoops": "^2.0",
    "fzaninotto/faker": "^1.4",
    "mockery/mockery": "^1.0",
    "nunomaduro/collision": "^2.0",
    "phpunit/phpunit": "^7.0"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "siesta\\application\\": "application/",
      "siesta\\domain\\": "domain/",
      "siesta\\infrastructure\\": "infrastructure/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "dont-discover": [
      ]
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true,
    "allow-plugins": {
      "kylekatarnls/update-helper": true
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
